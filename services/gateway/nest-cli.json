{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [
            ".dto.ts"
          ]
        }
      }
    ],
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true
  }
}